 v-model=\"booking.fullName\" label=\"Fullname\" outlined></v-text-field>\r\n        <v-text-field v-model=\"booking.email\" label=\"Email\" outlined></v-text-field>\r\n        <v-text-field v-model=\"booking.phone\" label=\"Phone\" outlined></v-text-field>\r\n        <v-select\r\n          return-object\r\n          item-text=\"stateName\"\r\n          item-value=\"stateId\"\r\n          v-model=\"state.selected\"\r\n          :items=\"state.items\"\r\n          label=\"Select state\"\r\n          solo\r\n        ></v-select>\r\n        <v-divider></v-divider>\r\n        <p\r\n          style=\"font-size: 1rem; font-weight: 700; margin: 20px 0 10px 0; color: #222\"\r\n        >Damage Policy</p>\r\n        <p\r\n          style=\"color: #555; font-size: .875rem;\"\r\n        >You will be responsible for any damage to the rental property caused by you or your party during your stay.</p>\r\n        <p\r\n          style=\"font-size: 1rem; font-weight: 700; margin: 20px 0 10px 0; color: #222\"\r\n        >House's rules</p>\r\n        <p style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\">ID/Passport or Deposit Required.</p>\r\n        <p style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\">Avoid making noises after 10 pm.</p>\r\n        <p style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\">No shoes in house.</p>\r\n        <p style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\">No smoking in shared spaces and common areas.</p>\r\n        <v-divider></v-divider>\r\n        <v-btn class=\"primary\" height=\"40\" v-on:click=\"requestToBook\" style=\"margin-top: 20px;\">Payment</v-btn>\r\n      </v-col>\r\n      <v-col cols=\"12\" lg=\"4\" style=\"background-color: grey; margin-top: 10px;\"></v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    bookingInfo: {\r\n      numberOfGuest: {\r\n        type: Number,\r\n        required: true\r\n      },\r\n      fromDate: {\r\n        type: String,\r\n        required: true\r\n      },\r\n      toDate: {\r\n        type: String,\r\n        required: true\r\n      },\r\n      classPK: {\r\n        type: String,\r\n        required: true\r\n      },\r\n      className: { type: String, required: true }\r\n    }\r\n  },\r\n  name: \"payment-view\",\r\n  data() {\r\n    return {\r\n      booking: {\r\n        numberOfGuest: null,\r\n        fromDate: null,\r\n        toDate: null,\r\n        classPK: null,\r\n        className: null,\r\n        fullName: null,\r\n        email: null,\r\n        phone: null,\r\n        stateId: null,\r\n        stateName: null\r\n      },\r\n      state: {\r\n        selected: { stateId: 1 , stateName: \"Viá»‡t Nam\"},\r\n        items: []\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    requestToBook() {\r\n      let vm = this;\r\n      vm.booking.numberOfGuest = vm.bookingInfo.numberOfGuest;\r\n      vm.booking.fromDate = vm.bookingInfo.fromDate;\r\n      vm.booking.toDate = vm.bookingInfo.toDate;\r\n      vm.booking.classPK = vm.bookingInfo.classPK;\r\n      vm.booking.className = vm.bookingInfo.className;\r\n      vm.booking.stateId = vm.state.selected.stateId;\r\n      vm.booking.stateName = vm.state.selected.stateName;\r\n      vm.$axios\r\n        .post(`http://localhost:8080/booking/api/v1/booking/home`, vm.booking)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          if (response.data.code === \"00\") {\r\n            if (!window.vnpay) {\r\n              window.location.href = response.data.data;\r\n            }\r\n            return false;\r\n          } else {\r\n            alert(response.data.message);\r\n          }\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  },\r\n  beforeCreate: function() {\r\n    let vm = this;\r\n    vm.$axios\r\n      .get(`http://localhost:8080/booking/api/v1/states`, {\r\n        params: { start: 0, end: 5000 }\r\n      })\r\n      .then(response => {\r\n        vm.state.items = response.data.content;\r\n      })\r\n      .catch(e => {\r\n        console.log(e);\r\n      });\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views/home"}]}