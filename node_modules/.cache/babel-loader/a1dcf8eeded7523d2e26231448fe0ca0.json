://localhost:8080/booking/api/v1/user/active/\" + _userId\r\n      };\r\n      vm.$axios(options)\r\n        .then(() => {\r\n          alert(\"done!\");\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  },\r\n  mounted: function() {\r\n    let vm = this;\r\n    vm.$axios\r\n      .get(`http://localhost:8080/booking/api/v1/users`, {\r\n        headers: {\r\n          Authorization: localStorage.getItem(\"jwtToken\")\r\n        },\r\n        params: {\r\n          isEnabled: 2,\r\n          isHost: 1\r\n        }\r\n      })\r\n      .then(response => {\r\n        vm.hostUsers.data = response.data.content;\r\n      })\r\n      .catch(e => {\r\n        console.log(e);\r\n      });\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views/admin"}]}