>{{ item.title }}</v-btn>\r\n                </template>\r\n                <template v-else>\r\n                  <v-btn text small :to=\"item.router\">{{ item.title }}</v-btn>\r\n                </template>\r\n              </v-list-item-title>\r\n            </v-list-item>\r\n          </v-list>\r\n        </v-menu>\r\n      </template>\r\n    </v-app-bar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"HostAppBar\",\r\n  methods: {\r\n    goBack() {\r\n      this.$router.replace(process.env.BASE_URL);\r\n    },\r\n    logout() {\r\n      localStorage.clear();\r\n      this.$router.replace(process.env.BASE_URL);\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      user: {\r\n        userId: \"\",\r\n        username: \"\",\r\n        firstName: \"\",\r\n        lastName: \"\",\r\n        age: 0,\r\n        address: \"\",\r\n        email: \"\",\r\n        phone: \"\",\r\n        birthDay: \"\",\r\n        description: \"\",\r\n        avatar: \"\"\r\n      },\r\n      isSign: localStorage.getItem(\"isSign\"),\r\n      username: localStorage.getItem(\"username\"),\r\n      profileMenu: [\r\n        { title: \"Your Profile\", router: \"\" },\r\n        { title: \"Message\", router: \"\" },\r\n        { title: \"Your Setting\", router: \"\" },\r\n        { title: \"Logout\", router: \"\" }\r\n      ]\r\n    };\r\n  },\r\n  created: function() {\r\n    let vm = this;\r\n    let userId = localStorage.getItem(\"userId\");\r\n    vm.$axios\r\n      .get(`/booking/api/v1/user/` + userId, {\r\n        headers: {\r\n          Authorization: localStorage.getItem(\"jwtToken\")\r\n        }\r\n      })\r\n      .then(response => {\r\n        if (response.data) {\r\n          vm.user = response.data;\r\n        }\r\n      })\r\n      .catch(e => {\r\n        vm.errors.push(e);\r\n      });\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/layouts/host"}]}