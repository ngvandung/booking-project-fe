SAAA,GAAA,EAAA,CAAA,KAAA,CAAA,QAAA,CAAA,SAAA;AACA,MAAA,EAAA,CAAA,MAAA,CACA,IADA,iCACA,EAAA,CAAA,OADA,EACA;AACA,QAAA,OAAA,EAAA;AACA,UAAA,aAAA,EAAA,YAAA,CAAA,OAAA,CAAA,UAAA;AADA;AADA,OADA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,cAAA,CAAA,MAAA,CAAA,KAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA;;AACA,iBAAA,KAAA;AACA,SALA,MAKA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,sBAAA,CAAA;AACA,SAFA,MAEA,IAAA,CAAA,CAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,cAAA,CAAA;AACA;AACA,OAtBA;AAuBA;AArDA,GA1DA;AAiHA,EAAA,WAAA,EAAA,uBAAA;AACA,QAAA,EAAA,GAAA,IAAA;AACA,QAAA,OAAA,GAAA,EAAA,CAAA,WAAA,CAAA,OAAA;;AAEA,QAAA,CAAA,OAAA,EAAA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA,YAAA;AACA;;AAEA,IAAA,EAAA,CAAA,MAAA,CACA,GADA,2BACA;AACA,MAAA,MAAA,EAAA;AAAA,QAAA,KAAA,EAAA,CAAA;AAAA,QAAA,GAAA,EAAA;AAAA;AADA,KADA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,MAAA,EAAA,CAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,KANA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,KATA;AAUA,IAAA,EAAA,CAAA,MAAA,CACA,GADA,CACA,0BAAA,OADA,EACA;AACA,MAAA,OAAA,EAAA;AACA,QAAA,aAAA,EAAA,YAAA,CAAA,OAAA,CAAA,UAAA;AADA;AADA,KADA,EAMA,IANA;AAAA,yEAMA,iBAAA,QAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,EAAA,CAAA,OAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CADA;;AAAA;AACA,gBAAA,KADA;AAEA,gBAAA,EAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA;AACA,gBAAA,EAAA,CAAA,aAAA,CAAA,GAAA,GACA,qBAAA,KAAA,CAAA,CAAA,CADA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OANA;;AAAA;AAAA;AAAA;AAAA,SAYA,KAZA,CAYA,UAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,KAdA;AAeA;AAlJA,CAAA","sourcesContent":["<template>\r\n  <v-container id=\"payment-view\">\r\n    <h3 style=\"font-size: 1.75rem; font-weight: 900; margin-bottom: 20px;\">Your information</h3>\r\n    <v-row>\r\n      <v-col cols=\"12\" lg=\"8\">\r\n        <v-text-field v-model=\"booking.fullName\" label=\"Fullname\" outlined></v-text-field>\r\n        <v-text-field v-model=\"booking.email\" label=\"Email\" outlined></v-text-field>\r\n        <v-text-field v-model=\"booking.phone\" label=\"Phone\" outlined></v-text-field>\r\n        <v-select\r\n          return-object\r\n          item-text=\"stateName\"\r\n          item-value=\"stateId\"\r\n          v-model=\"state.selected\"\r\n          :items=\"state.items\"\r\n          label=\"Select state\"\r\n          solo\r\n        ></v-select>\r\n        <v-divider></v-divider>\r\n        <p\r\n          style=\"font-size: 1rem; font-weight: 700; margin: 20px 0 10px 0; color: #222\"\r\n        >Damage Policy</p>\r\n        <p\r\n          style=\"color: #555; font-size: .875rem;\"\r\n        >You will be responsible for any damage to the rental property caused by you or your party during your stay.</p>\r\n        <p\r\n          style=\"font-size: 1rem; font-weight: 700; margin: 20px 0 10px 0; color: #222\"\r\n        >House's rules</p>\r\n        <p\r\n          style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\"\r\n        >ID/Passport or Deposit Required.</p>\r\n        <p\r\n          style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\"\r\n        >Avoid making noises after 10 pm.</p>\r\n        <p style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\">No shoes in house.</p>\r\n        <p\r\n          style=\"color: #555; font-size: .875rem; margin-bottom: 5px;\"\r\n        >No smoking in shared spaces and common areas.</p>\r\n        <v-divider></v-divider>\r\n        <v-btn\r\n          class=\"primary\"\r\n          height=\"40\"\r\n          v-on:click=\"requestToBook\"\r\n          style=\"margin-top: 20px;\"\r\n        >Payment</v-btn>\r\n      </v-col>\r\n      <v-col cols=\"12\" lg=\"4\" class=\"target-payment-info\">\r\n        <v-card class=\"mx-auto my-12\">\r\n          <v-img height=\"200\" :src=\"targetPayment.avt\"></v-img>\r\n          <v-card-title class=\"title text-truncate\">{{targetPayment.name}}</v-card-title>\r\n          <v-card-text>\r\n            <v-row align=\"center\" class=\"mx-0\">\r\n              <v-rating :value=\"4.5\" color=\"amber\" dense half-increments readonly size=\"14\"></v-rating>\r\n              <div class=\"grey--text ml-4\">4.5 (413)</div>\r\n            </v-row>\r\n            <div\r\n              class=\"my-4 subtitle-1 font-weight-bold\"\r\n            >{{formatPrice(targetPayment.price)}}$/night</div>\r\n            <div\r\n              class=\"text-truncate\"\r\n            >{{targetPayment.villageName}}, {{targetPayment.districtName}}, {{targetPayment.cityName}}, {{targetPayment.stateName}}</div>\r\n          </v-card-text>\r\n          <v-divider class=\"mx-4\"></v-divider>\r\n          <v-card-text>{{targetPayment.maxGuest}} guests • {{targetPayment.livingroom}} Livingroom • {{targetPayment.bathroom}} Bathroom • {{targetPayment.bedroom}} Bedroom</v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    bookingInfo: {\r\n      numberOfGuest: {\r\n        type: Number,\r\n        required: true\r\n      },\r\n      fromDate: {\r\n        type: String,\r\n        required: true\r\n      },\r\n      toDate: {\r\n        type: String,\r\n        required: true\r\n      },\r\n      classPK: {\r\n        type: String,\r\n        required: true\r\n      },\r\n      className: { type: String, required: true }\r\n    }\r\n  },\r\n  name: \"payment-view\",\r\n  data() {\r\n    return {\r\n      booking: {\r\n        numberOfGuest: null,\r\n        fromDate: null,\r\n        toDate: null,\r\n        classPK: null,\r\n        className: null,\r\n        fullName: null,\r\n        email: null,\r\n        phone: null,\r\n        stateId: null,\r\n        stateName: null\r\n      },\r\n      state: {\r\n        selected: { stateId: 1, stateName: \"Việt Nam\" },\r\n        items: []\r\n      },\r\n      targetPayment: {\r\n        name: \"\",\r\n        typeName: \"\",\r\n        stateName: \"\",\r\n        cityName: \"\",\r\n        districtName: \"\",\r\n        villageName: \"\",\r\n        linkGoogleMap: \"\",\r\n        price: \"\",\r\n        bedroom: \"\",\r\n        livingroom: \"\",\r\n        bathroom: \"\",\r\n        maxGuest: \"\",\r\n        avt: \"\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    formatPrice(value) {\r\n      let val = (value / 1).toFixed().replace(\".\", \",\");\r\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\r\n    },\r\n    callAPI(homeId) {\r\n      let vm = this;\r\n      return new Promise((resolve, reject) => {\r\n        vm.$axios\r\n          .get(\r\n            `/booking/api/v1/image/com.booking.model.Home/` +\r\n              homeId\r\n          )\r\n          .then(function(response) {\r\n            resolve(response.data.data);\r\n          })\r\n          .catch(err => {\r\n            reject(err);\r\n          });\r\n      });\r\n    },\r\n    requestToBook() {\r\n      let vm = this;\r\n      vm.booking.numberOfGuest = vm.bookingInfo.numberOfGuest;\r\n      vm.booking.fromDate = vm.bookingInfo.fromDate;\r\n      vm.booking.toDate = vm.bookingInfo.toDate;\r\n      vm.booking.classPK = vm.bookingInfo.classPK;\r\n      vm.booking.className = vm.bookingInfo.className;\r\n      vm.booking.stateId = vm.state.selected.stateId;\r\n      vm.booking.stateName = vm.state.selected.stateName;\r\n      vm.$axios\r\n        .post(`/booking/api/v1/booking/home`, vm.booking, {\r\n          headers: {\r\n            Authorization: localStorage.getItem(\"jwtToken\")\r\n          }\r\n        })\r\n        .then(response => {\r\n          if (response.data.code === \"00\") {\r\n            if (!window.vnpay) {\r\n              window.location.href = response.data.data;\r\n            }\r\n            return false;\r\n          } else {\r\n            alert(response.data.message);\r\n          }\r\n        })\r\n        .catch(e => {\r\n          if (e.response.status == 403) {\r\n            alert(\"You can't your house\");\r\n          } else if (e.response.status == 400) {\r\n            alert(\"It's renting\");\r\n          }\r\n        });\r\n    }\r\n  },\r\n  beforeMount: function() {\r\n    let vm = this;\r\n    let classPK = vm.bookingInfo.classPK;\r\n\r\n    if (!classPK) {\r\n      this.$router.replace(\"/error/403\");\r\n    }\r\n\r\n    vm.$axios\r\n      .get(`/booking/api/v1/states`, {\r\n        params: { start: 0, end: 5000 }\r\n      })\r\n      .then(response => {\r\n        vm.state.items = response.data.content;\r\n      })\r\n      .catch(e => {\r\n        console.log(e);\r\n      });\r\n    vm.$axios\r\n      .get(`/booking/api/v1/home/` + classPK, {\r\n        headers: {\r\n          Authorization: localStorage.getItem(\"jwtToken\")\r\n        }\r\n      })\r\n      .then(async response => {\r\n        let image = await vm.callAPI(response.data.homeId);\r\n        vm.targetPayment = response.data;\r\n        vm.targetPayment.avt =\r\n          \"/booking/images/\" + image[0];\r\n      })\r\n      .catch(e => {\r\n        console.log(e);\r\n      });\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.target-payment-info .my-12 {\r\n  margin-top: 0 !important;\r\n  margin-bottom: 0 !important;\r\n}\r\n</style>"],"sourceRoot":"src/views/common"}]}