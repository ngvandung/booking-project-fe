     }\r\n    };\r\n  },\r\n  methods: {\r\n    activeHome(_homeId) {\r\n      let vm = this;\r\n      const options = {\r\n        method: \"PUT\",\r\n        headers: { Authorization: localStorage.getItem(\"jwtToken\") },\r\n        url: \"http://localhost:8080/booking/api/v1/home/active/\" + _homeId\r\n      };\r\n      vm.$axios(options)\r\n        .then(() => {\r\n          alert(\"done!\");\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  },\r\n  mounted: function() {\r\n    let vm = this;\r\n    var query = {\r\n      indice: \"Home\",\r\n      query: {\r\n        bool: {\r\n          must: [\r\n            {\r\n              match: {\r\n                _type: \"Home\"\r\n              }\r\n            },\r\n            {\r\n              match: {\r\n                isActive: 2\r\n              }\r\n            }\r\n          ]\r\n        }\r\n      }\r\n    };\r\n\r\n    this.$axios\r\n      .post(`http://localhost:8080/booking/api/v1/_search`, query, {\r\n        headers: {\r\n          Authorization: \"Basic \" + localStorage.getItem(\"token\")\r\n        }\r\n      })\r\n      .then(response => {\r\n        vm.homes.data = response.data.hits.hits;\r\n      })\r\n      .catch(e => {\r\n        console.log(e);\r\n      });\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views/admin"}]}