d: true\r\n      },\r\n      classPK: {\r\n        type: String,\r\n        required: true\r\n      },\r\n      className: { type: String, required: true }\r\n    }\r\n  },\r\n  name: \"payment-view\",\r\n  data() {\r\n    return {\r\n      booking: {\r\n        numberOfGuest: null,\r\n        fromDate: null,\r\n        toDate: null,\r\n        classPK: null,\r\n        className: null,\r\n        fullName: null,\r\n        email: null,\r\n        phone: null,\r\n        stateId: null,\r\n        stateName: null\r\n      },\r\n      state: {\r\n        selected: { stateId: 1 , stateName: \"Viá»‡t Nam\"},\r\n        items: []\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    requestToBook() {\r\n      let vm = this;\r\n      vm.booking.numberOfGuest = vm.bookingInfo.numberOfGuest;\r\n      vm.booking.fromDate = vm.bookingInfo.fromDate;\r\n      vm.booking.toDate = vm.bookingInfo.toDate;\r\n      vm.booking.classPK = vm.bookingInfo.classPK;\r\n      vm.booking.className = vm.bookingInfo.className;\r\n      vm.booking.stateId = vm.state.selected.stateId;\r\n      vm.booking.stateName = vm.state.selected.stateName;\r\n      vm.$axios\r\n        .post(`http://localhost:8080/booking/api/v1/booking/home`, vm.booking)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          if (response.data.code === \"00\") {\r\n            if (!window.vnpay) {\r\n              window.location.href = response.data.data;\r\n            }\r\n            return false;\r\n          } else {\r\n            alert(response.data.message);\r\n          }\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  },\r\n  beforeCreate: function() {\r\n    let vm = this;\r\n    vm.$axios\r\n      .get(`http://localhost:8080/booking/api/v1/states`, {\r\n        params: { start: 0, end: 5000 }\r\n      })\r\n      .then(response => {\r\n        vm.state.items = response.data.content;\r\n      })\r\n      .catch(e => {\r\n        console